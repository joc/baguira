function showDiv() {

div = document.getElementById("boxN");

div.style.display = " ";

}

function closeDiv() {

div = document.getElementById("boxN");

div.style.display="none";

}

function demoDisplay()
{
document.getElementById("p1").style.display="";
}

function demoVisibility()
{
document.getElementById("p2").style.visibility="hidden";
}
