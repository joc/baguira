$(document).ready(function() {

	$("#vid").click(function() {
					$.fancybox.open({
						href : 'multimedia/videos',
						type : 'iframe',
						padding : 8 //borderline size
					});
				});
				
	$("#ani").click(function() {
					$.fancybox.open({
						href : 'multimedia/animaciones',
						type : 'iframe',
						padding : 8 //borderline size
					});
				});

});
