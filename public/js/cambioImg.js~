var i=1;
function cambio_uno(){
	if (i==1)
	{ 
	document.getElementById("uno").src="img/menu/mFotoB.png";
	i=2;
	}
	else
	{
	document.getElementById("uno").src="img/menu/mFotoA.png";
	i=1;
	}
}

